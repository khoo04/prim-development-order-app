{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "apility/laravel-fcm": "^1.5",
        "arcanedev/log-viewer": "7.x",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "2.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "2.15.0",
        "guzzlehttp/guzzle": "^7.8",
        "kawankoding/laravel-fcm": "^0.2.8",
        "kreait/firebase-php": "5.0",
        "kreait/laravel-firebase": "^2.2",
        "laravel/framework": "^7.29",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.5",
        "laravel/ui": "2.4",
        "maatwebsite/excel": "^3.1",
        "pragmarx/countries": "^0.7.2",
        "pusher/pusher-php-server": "~4.0",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/laravel-permission": "^3.18",
        "stripe/stripe-php": "^7.67",
        "symfony/symfony": "5.4.x",
        "yajra/laravel-datatables-oracle": "^9.15"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
            
        ],
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
